<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fred.qcm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.fred.qcm.QuestionnaireActivity"
            android:label="@string/app_name" >
        </activity>

        <service
            android:name="com.example.fred.qcm.GenerateQuestionnaire"
            android:exported="false" >
        </service>

        <activity
            android:name="com.example.fred.qcm.ProfilActivity"
            android:label="@string/title_activity_profil" >
        </activity>

        <service
            android:name="com.example.fred.qcm.GenerateProfil"
            android:exported="false" >
        </service>

        <activity
            android:name="com.example.fred.qcm.StartActivity"
            android:label="@string/title_activity_start" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.example.fred.qcm.LoadProfil"
            android:exported="false" >
        </service>
    </application>

</manifest>